num = int(input("Enter a number: "))
num_str = str(num)
power = len(num_str)
sum_of_digits = 0

for digit in num_str:
    sum_of_digits += int(digit) ** power
else:
    if sum_of_digits == num:
        print(f"{num} is an Armstrong number.")
    else:
        print(f"{num} is not an Armstrong number.")
